{
  "edit_predictions": {
    "mode": "subtle",
    "copilot": {
      "proxy": null,
      "proxy_no_verify": null
    },
    "enabled_in_text_threads": false
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "agent": {
    "always_allow_tool_actions": false,
    "default_profile": "write",
    "inline_assistant_model": {
      "provider": "anthropic",
      "model": "claude-3-7-sonnet-latest"
    },
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-opus-4-1"
    },
  },
  "languages": {
    "Markdown": {
      "show_edit_predictions": false,
      "soft_wrap": "editor_width"
    }
  },
  "theme": "Ayu Mirage",
  "base_keymap": "VSCode",
  "buffer_font_size": 13,
  "format_on_save": "on",
  "terminal": {
    "env": {
      "PATH": "/Users/duncan/.flox/run/aarch64-darwin.default.run/bin:$PATH"
    }
  }
}
