{
  "edit_predictions": {
    "mode": "subtle",
    "copilot": {
      "proxy": null,
      "proxy_no_verify": null
    },
    "enabled_in_text_threads": false
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "agent": {
    "always_allow_tool_actions": true,
    "default_profile": "write",
    "inline_assistant_model": {
      "provider": "zed.dev",
      "model": "claude-opus-4-1"
    },
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-opus-4-1"
    }
  },
  "languages": {
    "Markdown": {
      "show_edit_predictions": false,
      "soft_wrap": "editor_width"
    }
  },
  "theme": "Ayu Mirage",
  "base_keymap": "VSCode",
  "buffer_font_size": 13,
  "format_on_save": "on",
  "file_scan_exclusions": [
    "**/node_modules",
    ".DS_Store",
    ".git",
    ".wrangler",
    ".zed",
    ".claude",
    ".flox",
    "dist",
    "build",
    ".cache",
    "nix"
  ]
}
